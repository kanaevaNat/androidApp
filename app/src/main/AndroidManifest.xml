<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kanaeva.mobilecursach">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".ForgotPasswordActivity"></activity>
        <activity android:name=".EditUserActivity" />
        <activity android:name=".StatActivity" />
        <activity android:name=".Registration" />
        <activity android:name=".TestResult" />
        <activity android:name=".Personal" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".Question" />
        <activity android:name=".TestDescriptionActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>